{
	"Name": "gcServer",
	"ArtifactId": "server",
	"GroupId": "com.growcontrol",
	"Version": "4.0.0",
	"Props": {
		"pxnVersion": "4.0.0"
	},
	"Versions": {
		"jcl": "2.8",
		"SnakeYAML": "1.26"
	},
	"Org-Name": "GrowControl",
	"URL": "https://growcontrol.com",
	"GitBlit-Host": "git.poixson.com",
	"GitBlit-Repo": "gc!GrowControl",
	"Main-Class": "com.growcontrol.server.Main",
	"Shade": "yes",
	"Shade-Filters": [
		{
			"GroupArtifact": "com.growcontrol:gccommon",
			"Excludes": [
				"app.properties",
				"META-INF/MANIFEST.MF"
			]
		},
		{
			"GroupArtifact": "io.netty:netty-*",
			"Excludes": [
				"META-INF/MANIFEST.MF",
				"META-INF/io.netty.versions.properties"
			]
		},
		{
			"GroupArtifact": [
				"com.poixson:*",
				"org.yaml:snakeyaml:",
				"org.xeustechnologies:jcl-core",
				"org.quartz-scheduler:quartz",
				"org.fusesource.jansi:jansi",
				"org.jline:jline",
				"org.slf4j:*",
				"org.kamranzafar.commons:*",
				"com.mchange:*",
				"com.zaxxer:*",
				"cglib:*",
				"org.objenesis:objenesis"
			],
			"Excludes": [
				"META-INF/MANIFEST.MF"
			]
		}
	],
	"Deps": {
		"pxnCommon-Core": {
			"GroupId": "com.poixson",
			"ArtifactId": "common-core",
			"Version": "${pxnVersion}"
		},
		"pxnCommon-App": {
			"GroupId": "com.poixson",
			"ArtifactId": "common-app",
			"Version": "${pxnVersion}"
		},
		"pxnCommon-Shell": {
			"GroupId": "com.poixson",
			"ArtifactId": "common-shell",
			"Version": "${pxnVersion}"
		},
		"pxnCommon-Loggers": {
			"GroupId": "com.poixson",
			"ArtifactId": "common-loggers",
			"Version": "${pxnVersion}"
		},
		"pxnCommon-Config": {
			"GroupId": "com.poixson",
			"ArtifactId": "common-config",
			"Version": "${pxnVersion}"
		},
		"pxnCommon-Plugin": {
			"GroupId": "com.poixson",
			"ArtifactId": "common-plugin",
			"Version": "${pxnVersion}"
		},
		"pxnCommon-Scheduler": {
			"GroupId": "com.poixson",
			"ArtifactId": "common-scheduler",
			"Version": "${pxnVersion}"
		},
		"pxnCommon-Net": {
			"GroupId": "com.poixson",
			"ArtifactId": "common-net",
			"Version": "${pxnVersion}"
		},
		"gcCommon": {
			"GroupId": "com.growcontrol",
			"ArtifactId": "gccommon",
			"Version": "${project.version}"
		},
		"jcl": {
			"GroupId": "org.xeustechnologies",
			"ArtifactId": "jcl-core"
		},
		"SnakeYAML": {
			"GroupId": "org.yaml",
			"ArtifactId": "snakeyaml"
		}
	}
}
